# Converting the tibble to long format for easier plotting
a_long <- a %>%
pivot_longer(cols = everything(), names_to = "question", values_to = "response")
# Calculating the median for each question
medians <- a_long %>%
group_by(question) %>%
summarize(median_response = median(response, na.rm = TRUE))
# Merging the median information back to the long format data
a_long <- a_long %>%
left_join(medians, by = "question")
# Plotting the responses with medians in the title
ggplot(a_long, aes(x = response)) +
geom_bar() +
facet_wrap(~ question, scales = "free_x", ncol = 1, labeller = labeller(question = function(x) {
paste(x, "(Median:", medians$median_response[medians$question == x], ")")
})) +
theme_minimal() +
labs(title = "Survey Responses", x = "Response", y = "Count")
